{
    "name": "@via-profit-services/permissions",
    "support": "https://via-profit.ru",
    "version": "1.0.0",
    "description": "Via Profit Permissions Schema",
    "main": "dist/index.js",
    "engines": {
        "node": ">= 12.8.3",
        "npm": ">= 6.14.8"
    },
    "keywords": [
        "graphql",
        "via-profit",
        "permissions"
    ],
    "scripts": {
        "dist": "yarn lint && yarn build:dist",
        "build:dev": "cross-env NODE_ENV=development webpack --config ./webpack/webpack-config-dev.ts",
        "build:dist": "webpack --config ./webpack/webpack-config-prod.ts",
        "build:migrations": "webpack --config ./webpack/webpack-config-migrations.ts",
        "start": "yarn build:dev --watch",
        "debug": "cross-env DEBUG=true yarn build:dev --watch",
        "analyze": "cross-env ANALYZE=true yarn build:dist",
        "test": "jest",
        "lint": "eslint --ext .ts ./src/ --fix .",
        "migrate:latest": "yarn knex migrate:latest --knexfile ./.knex/knexfile.js",
        "migrate:rollback": "yarn knex migrate:rollback --knexfile ./.knex/knexfile.js",
        "migrate:up": "yarn knex migrate:up --knexfile ./.knex/knexfile.js",
        "migrate:down": "yarn knex migrate:down --knexfile ./.knex/knexfile.js",
        "migrate:make": "yarn knex migrate:make --migrations-directory ./src/database/migrations -x ts",
        "postversion": "git push --tags",
        "release:patch": "yarn version --patch",
        "release:minor": "yarn version --minor",
        "release:major": "yarn version --major",
        "release:prerelease": "yarn version --prerelease"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:via-profit-services/permissions.git"
    },
    "author": {
        "name": "Via Profit",
        "email": "promo@via-profit.ru",
        "url": "https://dev.via-profit.ru"
    },
    "contributors": [
        "Vasily Novosad <delhsmail@gmail.com>",
        "Pavel Natalin <trubonru@gmail.com>"
    ],
    "license": "MIT",
    "devDependencies": {
        "@graphql-tools/schema": "^7.1.2",
        "@graphql-tools/utils": "^7.1.2",
        "@types/express": "^4.17.9",
        "@types/node": "^14.14.10",
        "@types/nodemon": "^1.19.0",
        "@types/webpack": "^4.41.25",
        "@types/webpack-bundle-analyzer": "^3.9.0",
        "@types/webpack-merge": "^4.1.5",
        "@types/webpack-node-externals": "^2.5.0",
        "@typescript-eslint/eslint-plugin": "^4.9.1",
        "@typescript-eslint/parser": "^4.9.1",
        "@via-profit-services/core": "file://home/dhs/Projects/@via-profit-services/core/package-1.0.0-rc.1.tgz",
        "@via-profit-services/eslint-config": "^0.1.11",
        "cross-env": "^6.0.3",
        "dataloader": "^2.0.0",
        "dotenv": "^8.2.0",
        "eslint": "^7.12.1",
        "eslint-config-prettier": "^6.15.0",
        "eslint-config-standard": "^16.0.1",
        "eslint-import-resolver-node": "^0.3.4",
        "eslint-import-resolver-webpack": "^0.13.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-react": "^7.21.5",
        "eslint-plugin-standard": "^4.0.2",
        "express": "^4.17.1",
        "graphql": "^15.4.0",
        "graphql-tag": "^2.11.0",
        "ioredis": "^4.19.4",
        "nodemon": "^2.0.7",
        "nodemon-webpack-plugin": "^4.3.2",
        "prettier": "^2.1.2",
        "raw-loader": "^4.0.2",
        "supports-color": "^8.1.0",
        "ts-jest": "^24.2.0",
        "ts-loader": "^6.2.1",
        "ts-node": "^8.6.2",
        "typescript": "^4.1.2",
        "webpack": "^5.9.0",
        "webpack-bundle-analyzer": "^4.2.0",
        "webpack-cli": "^4.2.0",
        "webpack-merge": "^5.4.0",
        "winston": "^3.3.3",
        "winston-daily-rotate-file": "^4.5.0"
    },
    "peerDependencies": {
        "@via-profit-services/core": "^1.0.0",
        "graphql": "*",
        "winston": "*",
        "winston-daily-rotate-file": "*"
    }
}
